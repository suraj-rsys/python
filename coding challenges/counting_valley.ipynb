    "def countingvalley(s):\n",
    "    level = 0\n",
    "    valley = 0\n",
    "\n",
    "    for i in s:\n",
    "        if i == \"U\":\n",
    "            level += 1\n",
    "        elif i == \"D\":\n",
    "            level -= 1\n",
    "        if level == 0 and i == \"U\":\n",
    "            valley += 1\n",
    "    return valley\n",
    "\n",
    "print(countingvalley(\"DDUUDDUDUUUD\"))"
   ]
